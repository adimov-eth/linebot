{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "Nodenext",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "sourceMap": true,
    "lib": ["ES2023"],
    "skipLibCheck": true,
    "rootDir": "src",
    "outDir": "dist",
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
  }
}
